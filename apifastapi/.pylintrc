[MASTER]
extension-pkg-allow-list=pydantic
init-hook="from pylint.config import find_pylintrc; import os, sys; sys.path.append(os.path.dirname(find_pylintrc()))"

[MESSAGES CONTROL]
disable=
    C0116,
    C0111,
    E1101,
    W1203,
    W0718,

[format]
max-line-length=88

[MISCELLANEOUS]
notes=FIXME

[VARIABLES]
additional-builtins=
    spark

[HEADER]
good-names=db
