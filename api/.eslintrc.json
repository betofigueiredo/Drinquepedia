{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2020": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "no-tabs": ["error", { "allowIndentationTabs": true }],
        "indent": ["error", "tab"],
        "object-curly-newline": [0]
    }
}
